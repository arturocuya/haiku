<script>
    m.haiku = [
        "The west wind whispered",
        "And touched the eyelids of spring:",
        "Her eyes, Primroses."
    ]
    m.index = 0
</script>
<Button
    :focus
    text={m.haiku[m.index]}
    on:buttonSelected={sub ()
        m.index = m.index < m.haiku.count() - 1 ? m.index + 1 : 0
        ? "buttonSelected: m.index", m.index
    end sub}
/>